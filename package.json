{
  "name": "express-es6-starter",
  "version": "1.0.0",
  "description": "A complete boilerplate for Express.js development including unit test and code coverage.",
  "main": "lib/index.js",
  "scripts": {
    "start": "nodemon lib/index.js --exec babel-node --presets es2015,stage-0",
    "build": "babel lib -d dist",
    "serve": "node dist/index.js",
    "test": "node_modules/.bin/babel-node node_modules/.bin/babel-istanbul cover node_modules/.bin/_mocha"
  },
  "pre-commit": [
    "test"
  ],
  "author": "Stephane P. Pericat <contact@shortwavapp.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/stephanepericat/express-es6-starter"
  },
  "dependencies": {
    "express": "^4.14.0",
    "yahoo-finance-data": "^1.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-istanbul": "^0.11.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.11.6",
    "mocha": "^3.0.2",
    "nodemon": "^1.10.2",
    "pre-commit": "^1.1.3"
  }
}
